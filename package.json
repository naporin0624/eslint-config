{
  "name": "@naporin0624/eslint-config",
  "version": "0.10.0",
  "description": "eslint-config that @naporin0624 usually uses in his applications.",
  "keywords": [
    "eslint"
  ],
  "main": "index.js",
  "private": false,
  "files": [
    "index.js",
    "react.js",
    "README.md",
    "package.json"
  ],
  "author": "naporin0624",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "lint": "npm-run-all -s 'lint:*'",
    "lint:eslint": "eslint  .",
    "lint:prettier": "prettier --check .",
